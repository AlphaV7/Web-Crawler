{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "strongloop"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "max-len": [2, 120, 2, {"ignoreUrls": true}],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "handle-callback-err": "warn",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }]
  }
}
